#include <iostream>
#include <string>

using namespace std;

//assume small letter alphabet
char maximumOccuringAlphabet(string s) {
	char c = '\0';
	int maxCount = 0;
	int countArray[26] = {0};
	
	for(int i = 0; s[i] != '\0'; i++) {
		if((s[i] >= 'a' && s[i] <= 'z')) {
			int index = s[i] - 'a';
			countArray[index]++;
			if(maxCount < countArray[index]) {
				maxCount = countArray[index];
				c = 'a' + index;
			}
		}
	}
	return c;
}

int main() {
	// your code goes here
	string s = "test string";
	cout << maximumOccuringAlphabet(s);
	return 0;
}
